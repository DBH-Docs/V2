{
  "name": "personal-page",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "plugins/*"
  ],
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "yarn build:config && docusaurus start",
    "build": "yarn build:config && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "yarn build:config && docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "build:plugin": "yarn workspace plugin-blog-index build",
    "build:config": "tsc -p tsconfig-config.json",
    "lint": "eslint --cache \"**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@date-io/date-fns": "1.x",
    "@docusaurus/core": "2.0.0-beta.8",
    "@docusaurus/preset-classic": "2.0.0-beta.8",
    "@docusaurus/theme-live-codeblock": "2.0.0-beta.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.3",
    "@material-ui/pickers": "^3.2.10",
    "@mdx-js/react": "^1.6.21",
    "clsx": "^1.1.1",
    "color": "^4.0.1",
    "date-fns": "^2.19.0",
    "pure-react-carousel": "^1.27.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rehype-katex": "^5.0.0",
    "remark-math": "3.0.1",
    "uuidv4": "^6.2.6"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.8",
    "@tsconfig/docusaurus": "^1.0.3",
    "@types/color": "^3.0.2",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-jc": "^1.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "resolutions": {
    "reading-time": "2.0.0-1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx}": [
      "prettier --write"
    ]
  }
}
